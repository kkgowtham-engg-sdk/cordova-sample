<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moengage.sampleapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="PluginDemo">
        <param name="android-package" value="cordova-plugin-demo.PluginDemo" />
    </feature>
    <feature name="DemoPlugin">
        <param name="android-package" value="cordova.plugin.demo.DemoPlugin" />
    </feature>
    <feature name="uiplugin">
        <param name="android-package" value="com.moe.uiplugin.uiplugin" />
    </feature>
    <feature name="MoEngage">
        <param name="android-package" value="com.moengage.cordova.MoECordova" />
    </feature>
    <name>HelloCordova</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
